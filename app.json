{
    "name": "zoro-md-v1.0.1",
    "version":"1.0.0",
    "description": "zoro Multi Device whatsappbot",
    "logo": "https://i.imgur.com/0Eka4Gs.jpeg",
    "keywords": ["whatsapp"],
    "repository": "https://github.com/zokosir/ZORO-MD-V1.0.1",
    "website": "https://github.com/zokosir/ZORO-MD-V1.0.1",
    "success_url": "https://github.com/zokosir/ZORO-MD-V1.0.1",
    "stack": "container",
    "env": {
        "BOT_INFO": {
            "description": "botname;ownername;ownernumber;image url",
            "required": true,
            "value":"zoro;https://i.imgur.com/0Eka4Gs.jpeg"
        },
        "AUDIO_DATA": {
            "description": "audio package name",
            "required": true,
            "value":"zoro-md;ᴊᴀʀᴠɪꜱ;https://i.imgur.com/0Eka4Gs.jpeg"
        },
        "WARN_COUNT": {
            "description": "warn count",
            "required": true,
            "value":"3"
        },
        "SERVER": {
            "description": "delpoying server",
            "required": true,
            "value":"heroku"
        },
        "AUTO_STATUS_VIEWS": {
            "description": "auto status views",
            "required": true,
            "value":"false"
        },
        "MEDIA_DATA": {
            "description": "media data that uses in list",
            "required": true,
            "value":"ʟɪꜱᴛ ᴍᴇɴᴜ;zoro-md;https://i.imgur.com/0Eka4Gs.jpeg"
        },
        "HANDLER": {
            "description": "handler for bot. all so can be null or false",
            "required": true,
            "value":"."
        },
        "SESSION_ID": {
            "description": "session id",
            "required": true
        },
        "WORK_TYPE": {
            "description": "worker type public or private",
            "required": true,
            "value":"private"
        },
        "HEROKU_APP_NAME": {
            "description": "heroku app name, same as above entered",
            "required": true
        },
        "HEROKU_API_KEY": {
            "description": "Heroku account api key, https://dashboard.heroku.com/account",
            "required": true
        },
        "STICKER_PACKNAME": {
            "description": "sticker package name",
            "required": true,
            "value":"zoroɪ;md"
        },
        "SUDO": {
            "description": "owner number of the bot",
            "required": false,
            "value":"919778158839"
        }
    },
      "addons": [{
      "plan": "heroku-postgresql"
    }],
    "buildpacks": [{
      "url": "heroku-community/apt"
      },
      {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
      }]
  }
  
